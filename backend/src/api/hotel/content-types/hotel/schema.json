{
  "kind": "collectionType",
  "collectionName": "hotels",
  "info": {
    "singularName": "hotel",
    "pluralName": "hotels",
    "displayName": "Hotel",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "description": {
      "type": "blocks"
    },
    "gallery": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "regions": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::region.region"
    },
    "latitude": {
      "type": "float"
    },
    "longitude": {
      "type": "float"
    },
    "address": {
      "type": "string"
    },
    "phone": {
      "type": "string",
      "regex": "^((8|\\+7)[\\- ]?)?(\\(?\\d{3}\\)?[\\- ]?)?[\\d\\- ]{7,10}$"
    },
    "hotel_category": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::hotel-category.hotel-category"
    },
    "Rooms": {
      "type": "dynamiczone",
      "components": [
        "hotels.hotel-rooms"
      ]
    },
    "email": {
      "type": "email"
    }
  }
}
